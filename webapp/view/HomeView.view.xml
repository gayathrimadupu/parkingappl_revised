                <m:Label text="" />
                <m:Button
                    id="idButtonAssign"
                    type="Unstyled"
                    class="buttonBlue"
                    text="Assign"
                    press="onAssignPress"
                    width="200px"
                />
            </layout:HorizontalLayout>
        </form:SimpleForm>
