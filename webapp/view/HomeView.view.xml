            <!-- Driver Name -->
            <layout:HorizontalLayout>
                <m:Label text="Driver Name" class="labelSpacing" />
                <m:Input
                    id="idDriverName"
                    placeholder="Enter Driver Name"
                    width="200px"
                    change="onDriverNameChange"
                />
            </layout:HorizontalLayout>

            <!-- Driver Mobile -->
            <layout:HorizontalLayout>
                <m:Label text="Driver Mobile" class="labelSpacing" />
                <m:Input
                    id="idDriverMobile"
                    placeholder="Enter Driver Mobile"
                    width="200px"
                    type="Number"
                    change="onMobileNumberChange"
                />
            </layout:HorizontalLayout>

            <!-- Vehicle Number -->
            <layout:HorizontalLayout>
                <m:Label text="Vehicle Number" class="labelSpacing" />
                <m:Input
                    id="idVehicleNumber"
                    placeholder="Enter Vehicle Number"
                    width="200px"
                    change="onVehicleNumberChange"
                />
            </layout:HorizontalLayout>

            <!-- Vendor Name -->
            <layout:HorizontalLayout>
                <m:Label text="Vendor Name" class="labelSpacing" />
                <m:Input
                    id="idVendorName"
                    placeholder="Enter Vendor Name"
                    width="200px"
                />
            </layout:HorizontalLayout>

            <!-- Delivery Type -->
            <layout:HorizontalLayout>
                <m:Label text="Delivery Type" class="labelSpacing" />
                <m:Select id="idTypeOfDelivery" width="200px">
                    <core:Item
                        key="Select"
                        text="Select"
                    />
                    <core:Item
                        key="InBound"
                        text="InBound"
                    />
                    <core:Item
                        key="OutBound"
                        text="OutBound"
                    />
                </m:Select>
            </layout:HorizontalLayout>

            <!-- Parking Slot -->
            <layout:HorizontalLayout>
                <m:Label text="Parking Slot" class="labelSpacing" />
                <m:Select
                    id="parkingLotSelect"
                    items="{path: '/ZPARKING_SLOTS_SSet', filters: [{path: 'Status', operator: 'EQ', value1: 'AVAILABLE'}]}"
                    change="onParkingLotSelectChange"
                    width="200px"
                >
                    <core:Item
                        key="{Slotnumbers}"
                        text="{Slotnumbers}"
                    />
                </m:Select>
            </layout:HorizontalLayout>

            <!-- Assign Button -->
