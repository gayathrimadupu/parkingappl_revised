<mvc:View
    controllerName="com.app.parkingappl.controller.HomeView"
    xmlns="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:ui5="sap.ui.webc.main"
    height="100%"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:ui="sap.ui.commons"
    xmlns:html="http://www.w3.org/1999/xhtml"
>
    <ObjectPageLayout
        id="idUserDataPage"
        enableLazyLoading="true"
        useIconTabBar="true"
        showTitleInHeaderContent="true"
        upperCaseAnchorBar="false"
        sectionChange="onSectionChange"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle
                id="idObjectPageDynamicHeaderTitle"
                class="title"
            >
                <expandedHeading>
                    <m:HBox
                        id="idExpandedHeadingBox"
                        width="100%"
                        justifyContent="SpaceBetween"
                        alignItems="Center"
                    >
                        <!-- Title on the left -->
                        <m:Title
                            id="idExpandedHeadingTitle"
                            text="Parking Management"
                            wrapping="true"
                            level="H4"
                        />

                        <!-- Buttons aligned to the right -->
                    </m:HBox>
                </expandedHeading>

                <actions>
                    <m:HBox
                        id="idButtonsBox"
                        alignItems="Center"
                    >
                       <m:Button
    id="idReservations"
    tooltip="Reservation Request"
    icon="sap-icon://request"
    press="onReservationsPress"
    class="whiteButton"
>
    <m:customData>
        <m:BadgeCustomData
            id="idbadghqwe"
           value=""
            visible="true"
            animation="Update"
        />
    </m:customData>
</m:Button>

<m:Button
    id="idDataVisualization"
    icon="sap-icon://pie-chart"
    tooltip="Data visualization"
    press="onDataAnalysisPress"
    class="whiteButton"
/>
                    </m:HBox>
                </actions>
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <sections>
            <ObjectPageSection
                titleUppercase="false"
                id="idBorrowBooksSection11"
                title="Details"
                class="Content"
            >
                <subSections>
                    <ObjectPageSubSection
                        id="idBorrowBooksSubSection11"
                        title="Lot Allocation"
                        class="blueBackground"
                    >
                        <blocks>
                            <layout:HorizontalLayout
                                id="idreservehbox"
                                alignItems="Center"
                                justifyContent="Center"
                                width="100%"
                                class="contentWrapper"
                            >
                                <form:SimpleForm
                                    id="idSimpleForm1"
                                    layout="ResponsiveGridLayout"
                                    editable="true"
                                    class="smallForm formBackground"
                                >
                                    <!-- Driver Name -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Driver Name:"
                                           
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Input
                                            id="idDriverName"
                                            placeholder="Enter Driver Name"
                                            width="200px"
                                            change="onDriverNameChange"
                                            class="inputColor"
                                        />
                                    </layout:HorizontalLayout>

                                    <!-- Driver Mobile -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Driver Mobile:"
                                           
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Input
                                            id="idDriverMobile"
                                            placeholder="Enter Driver Mobile"
                                            width="200px"
                                            type="Number"
                                            change="onMobileNumberChange"
                                            class="inputColor"
                                        />
                                    </layout:HorizontalLayout>

                                    <!-- Vehicle Number -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Vehicle  Num:"
                                            
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Input
                                            id="idVehicleNUmber"
                                            placeholder="Enter Vehicle Number"
                                            width="200px"
                                            change="onVehicleNumberChange"
                                            class="inputColor"
                                        />
                                    </layout:HorizontalLayout>

                                    <!-- Vendor Name -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Vendor Name:"
                                           
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Input
                                            id="idVendorName___"
                                            placeholder="Enter Vendor Name"
                                            width="200px"
                                            class="inputColor"
                                        />
                                    </layout:HorizontalLayout>

                                    <!-- Delivery Type -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Delivery Type:"
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Select
                                            id="idTypeOfDelivery"
                                            width="200px"
                                            class="inputColor"
                                        >
                                            <core:Item
                                                key="Select"
                                                text="Select"
                                            />
                                            <core:Item
                                                key="InBound"
                                                text="InBound"
                                            />
                                            <core:Item
                                                key="OutBound"
                                                text="OutBound"
                                            />
                                        </m:Select>
                                    </layout:HorizontalLayout>

                                    <!-- Parking Slot -->
                                    <layout:HorizontalLayout>
                                        <m:Label
                                            text="Parking Slot:"
                                            class="labelSpacing labelColor"
                                        />
                                        <m:Select
                                            id="parkingLotSelect"
                                            items="{path: '/ZPARKING_SLOTS_SSet', filters: [{path: 'Status', operator: 'EQ', value1: 'AVAILABLE'}]}"
                                            change="onParkingLotSelectChange"
                                            width="200px"
                                            class="inputColor"
                                        >
                                            <core:Item
                                                key="{Slotnumbers}"
                                                text="{Slotnumbers}"
                                            />
                                        </m:Select>
                                    </layout:HorizontalLayout>

                                    <!-- Assign Button -->
                                    <layout:HorizontalLayout>
                                        <m:Label text="" />
                                        <m:Button
                                            id="idButtonAssign"
                                            class="blueButton"
                                            type="Default"
                                            text="Assign"
                                            press="onAssignPress"
                                            width="200px"
                                        />
                                    </layout:HorizontalLayout>
                                </form:SimpleForm>
                            </layout:HorizontalLayout>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>

